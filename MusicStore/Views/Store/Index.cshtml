@model IEnumerable<MusicStore.Models.Genre>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Borwse Genres</h3>
<p>Select from @Model.Count()</p>
<ul>
    @foreach (var genre in Model)
    { 
        <li>@Html.ActionLink(genre.Name,"Browse",new {  genre = genre.Name })</li>
    }
</ul>
